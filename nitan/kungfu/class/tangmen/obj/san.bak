#include <ansi.h>
#include <weapon.h>
inherit THROWING;

void create()
{
        set_name(HIG"散花天女" NOR, ({"sanhua tianyu"}));
        if (clonep())
                set_default_object(__FILE__);
        else
        {
                set("long", "這是一枚四川唐門秘制的暗器散花天女，威力無比。\n");
                set("unit", "些");
                set("base_value", 2100);
                set("material", "steel");
                set("base_unit", "枚");
                set("base_weight", 10);            
        }
        set_amount(1);
        init_throwing(100);
        setup();
}
